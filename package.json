{
  "name": "simple-framework",
  "version": "1.0.0",
  "description": "简易框架",
  "dependencies": {
    "egg": "^2",
    "egg-http-error": "git+ssh://git@github.com/kiba-zhao/egg-http-error.git",
    "egg-http-logger": "git+ssh://git@github.com/kiba-zhao/egg-http-logger.git",
    "egg-params-inject": "git+ssh://git@github.com/kiba-zhao/egg-params-inject.git",
    "egg-router-simple": "git+ssh://git@github.com/kiba-zhao/egg-router-simple.git",
